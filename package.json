{
  "name": "gofmt",
  "description": "Format go source code with gofmt, goimports, or goreturns.",
  "keywords": [
    "go",
    "golang",
    "gofmt",
    "goimports",
    "goreturns",
    "format",
    "formatter"
  ],
  "main": "./lib/main",
  "version": "0.0.0",
  "repository": "https://github.com/joefitzgerald/gofmt",
  "license": "Apache-2.0",
  "engines": {
    "atom": ">=1.6.0 <2.0.0"
  },
  "author": "Joe Fitzgerald <jfitzgerald@pivotal.io> (http://github.com/joefitzgerald)",
  "homepage": "https://atom.io/packages/gofmt",
  "bugs": {
    "url": "https://github.com/joefitzgerald/gofmt/issues"
  },
  "dependencies": {
    "atom-package-deps": "^4.0.1"
  },
  "devDependencies": {
    "eslint": "^2.1.0",
    "babel-eslint": "^5.0.0",
    "eslint-config-standard": "^5.1.0",
    "eslint-plugin-standard": "^1.3.2",
    "eslint-plugin-promise": "^1.0.8"
  },
  "package-deps": [
    "go-config",
    "go-get"
  ],
  "consumedServices": {
    "go-config": {
      "versions": {
        "0.1.0": "consumeGoconfig"
      }
    },
    "go-get": {
      "versions": {
        "0.1.0": "consumeGoget"
      }
    }
  },
  "providedServices": {
    "linter": {
      "versions": {
        "1.0.0": "provide"
      }
    }
  }
}
